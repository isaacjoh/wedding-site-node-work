	<script>
		function initMap() {
			var seventh_place = {
				lat: 34.033493,
				lng: -118.232001
			};
			var map = new google.maps.Map(document.getElementById('map'), {
				zoom: 12,
				center: seventh_place
			});
			var marker = new google.maps.Marker({
				position: seventh_place,
				map: map
			});
		}
	</script>
